{
  "name": "@metamask/snaps-simulator",
  "version": "0.0.0",
  "private": true,
  "description": "A simulator for MetaMask Snaps, to be used for testing and development",
  "homepage": "https://github.com/MetaMask/snaps-simulator#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/snaps-simulator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/snaps-simulator.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "build:clean": "rimraf dist && yarn build",
    "postinstall": "simple-git-hooks",
    "lint": "yarn lint:eslint && yarn lint:constraints && yarn lint:misc --check && yarn lint:dependencies --check && yarn lint:changelog",
    "lint:changelog": "auto-changelog validate",
    "lint:constraints": "yarn constraints",
    "lint:dependencies": "depcheck && yarn dedupe",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:constraints --fix && yarn lint:misc --write && yarn lint:dependencies && yarn lint:changelog",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' '**/*.html' '!.yarnrc.yml' --ignore-path .gitignore --no-error-on-unmatched-pattern",
    "prepack": "./scripts/prepack.sh",
    "start": "webpack serve --mode development",
    "test": "jest && jest-it-up --margin 0.5",
    "test:watch": "jest --watch"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged && yarn dedupe"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "!(CHANGELOG).{json,yml,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.1.1",
    "@chakra-ui/react": "^2.6.1",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@ethersproject/units": "^5.7.0",
    "@metamask/base-controller": "^2.0.0",
    "@metamask/eth-json-rpc-middleware": "^11.0.0",
    "@metamask/key-tree": "^7.0.0",
    "@metamask/permission-controller": "^3.2.0",
    "@metamask/rpc-errors": "^5.1.1",
    "@metamask/rpc-methods": "^0.32.2",
    "@metamask/snaps-controllers": "^0.32.2",
    "@metamask/snaps-ui": "^0.32.2",
    "@metamask/snaps-utils": "^0.32.2",
    "@metamask/utils": "^5.0.2",
    "@reduxjs/toolkit": "^1.9.5",
    "date-fns": "^2.30.0",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^10.12.8",
    "json-rpc-engine": "^6.1.0",
    "json-rpc-middleware-stream": "^4.2.1",
    "monaco-editor": "^0.38.0",
    "pump": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-markdown": "^6.0.3",
    "react-monaco-editor": "^0.52.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.1",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.3.1",
    "@lavamoat/preinstall-always-fail": "^1.0.0",
    "@metamask/auto-changelog": "^3.1.0",
    "@metamask/eslint-config": "^11.0.1",
    "@metamask/eslint-config-browser": "^11.1.0",
    "@metamask/eslint-config-jest": "^11.0.0",
    "@metamask/eslint-config-nodejs": "^11.0.1",
    "@metamask/eslint-config-typescript": "^11.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@swc/core": "^1.3.56",
    "@swc/jest": "^0.2.26",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.23",
    "@types/pump": "^1.1.1",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "assert": "^2.0.0",
    "constants-browserify": "^1.0.0",
    "css-loader": "^6.7.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-it-up": "^2.0.2",
    "lint-staged": "^13.2.2",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "process": "^0.11.10",
    "react-refresh": "^0.14.0",
    "readable-stream": "^4.4.0",
    "redux-saga-test-plan": "^4.0.6",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.2",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.7.0",
    "typescript": "~4.8.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3",
    "webpackbar": "^5.0.2"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "@swc/core": true,
      "@pmmmwh/react-refresh-webpack-plugin>core-js-pure": false,
      "simple-git-hooks": true,
      "$root$": true,
      "@metamask/permission-controller>@metamask/controller-utils>ethereumjs-util>ethereum-cryptography>keccak": false,
      "@metamask/permission-controller>@metamask/controller-utils>ethereumjs-util>ethereum-cryptography>secp256k1": false,
      "@metamask/base-controller>@metamask/controller-utils>ethereumjs-util>ethereum-cryptography>keccak": false,
      "@metamask/base-controller>@metamask/controller-utils>ethereumjs-util>ethereum-cryptography>secp256k1": false
    }
  }
}
